<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="InventoryCleanApp.Pages.ResetPasswordPage"
             Title="ResetPasswordPage">
    <VerticalStackLayout Padding="25" Spacing="25">
        <Label 
            Text="Reset Page"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button Margin="0,50,0,0" 
        BackgroundColor="#4F3F9D"
        HeightRequest="60"
        Text="Logout"
        TextColor="#FBF6FD"
        Clicked="Clicked_logout_button"
        />
    </VerticalStackLayout>
</ContentPage>